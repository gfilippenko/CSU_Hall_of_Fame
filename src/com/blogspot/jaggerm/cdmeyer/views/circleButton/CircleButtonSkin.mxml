<!--- The default skin class for the Spark Button component.  

       @see spark.components.Button
        
      @langversion 3.0
      @playerversion Flash 10
      @playerversion AIR 1.5
      @productversion Flex 4
-->
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
             xmlns:s="library://ns.adobe.com/flex/spark" 
             xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
             minWidth="21" minHeight="21">
     
    <fx:Metadata>
        <![CDATA[ 
        [HostComponent("spark.components.Button")]
        ]]>
    </fx:Metadata>
    
    <fx:Script fb:purpose="styling">
        <![CDATA[         
            
                                 
        ]]>        
    </fx:Script>
        
    <!-- states -->
    <s:states>
        <s:State name="disabled" />
		<s:State name="down" />
		<s:State name="over" />
		<s:State name="up" />
    </s:states>
    
   
	
	<s:BitmapImage width="266" height="266"  
				   source.down="@Embed('../../../../../../resources/yellow_circle_down.png')" 
				   source.up="@Embed('../../../../../../resources/yellow_circle.png')" 
				   source.over="@Embed('../../../../../../resources/yellow_circle.png')"
				   source.disabled="@Embed('../../../../../../resources/yellow_circle_disabled.png')"/>
    

    <s:Label id="labelDisplay"
             textAlign="center"
             maxDisplayedLines="3"
             horizontalCenter="0" verticalCenter="1" verticalAlign="middle"
             left="10" right="10" top="2" bottom="2"
			 fontFamily="Swis721CnBT" fontWeight="bold" fontSize="36.22" color="0xffffff" color.disabled="666666">
    </s:Label>
    
</s:Skin>
